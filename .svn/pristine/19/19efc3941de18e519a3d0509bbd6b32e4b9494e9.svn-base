@import "../common";
body {}
.topBar("../../../img/topBack-ico.png");

.mui-content {
    padding-bottom: 2.5rem;
}

.sonsignInfo {
    padding: 0 0.75rem;
    margin-bottom: 0.625rem;
    margin-top: 0.625rem;
    background: #fff;
    .sonsignList {
        display: -webkit-box;
        display: -ms-flexbox;
        display: flex;
        -webkit-box-orient: vertical;
        -webkit-box-direction: normal;
        -ms-flex-direction: column;
        flex-direction: column;
        padding-top: 1.625rem;
        padding-bottom: 0.625rem;
        border-bottom: 0.0625rem solid #E6E6E6;
        .sonsignListItem {
            display: -webkit-box;
            display: -ms-flexbox;
            display: flex;
            -webkit-box-pack: justify;
            -ms-flex-pack: justify;
            justify-content: space-between;
            text-indent: 2.875rem;
            margin-bottom: 0.625rem;
            font-size: 0.8125rem;
            color: #333;
            p {
                font-size: 0.8125rem;
                color: #333;
            }
        }
    }
    .sonsignInfoFooter {
        padding: 0.9375rem 0 0.3125rem;
        p {
            margin-bottom: 0.3125rem;
            font-size: 0.75rem;
            color: #333;
        }
    }
}

.consumptionInfo {
    padding: 0 0.75rem;
    background: #fff;
    .consumptionItem {
        color: #333;
        display: -webkit-box;
        display: -ms-flexbox;
        display: flex;
        -webkit-box-flex: 1;
        -ms-flex: 1;
        flex: 1;
        width: 100%;
        -webkit-box-pack: justify;
        -ms-flex-pack: justify;
        justify-content: space-between;
        -webkit-box-align: center;
        -ms-flex-align: center;
        align-items: center;
        height: 2.75rem;
        border-bottom: 0.0625rem solid #E6E6E6;
        font-size: 0.75rem;
        span {
            -webkit-box-flex: 1;
            -ms-flex: 1;
            flex: 1;
        }
        span:last-child {
            -webkit-box-flex: 4;
            -ms-flex: 4;
            flex: 4;
        }
    }
}

.sure {
    position: fixed;
    display: -webkit-box;
    display: -ms-flexbox;
    display: flex;
    -webkit-box-align: center;
    -ms-flex-align: center;
    align-items: center;
    -webkit-box-pack: center;
    -ms-flex-pack: center;
    justify-content: center;
    bottom: 0;
    width: 100%;
    height: 2.5rem;
    font-size: 1.125rem;
    color: #fff;
    background: #333;
}

.infoTit {
    display: -webkit-box;
    display: -ms-flexbox;
    display: flex;
    -webkit-box-flex: 1;
    -ms-flex: 1;
    flex: 1;
    -webkit-box-pack: justify;
    -ms-flex-pack: justify;
    justify-content: space-between;
    -webkit-box-align: center;
    -ms-flex-align: center;
    align-items: center;
    height: 2.0625rem;
    font-size: 0.875rem;
    color: #333;
    border-bottom: 0.0625rem solid #E6E6E6;
    p {
        font-size: 0.875rem;
        color: #333;
    }
    .infoTitDate {
        color: #f54141;
    }
}

.addSuccess {
    position: absolute;
    top: 0;
    z-index: 99999;
    bottom: -17.18rem;
    left: 0;
    right: 0;
    width: 14.25rem;
    height: 6.68rem;
    margin: auto;
    color: #fff;
    border-radius: 0.56rem;
    background-color: #3d3d3d;
    opacity: 0;
    -webkit-transform: scale(0);
    transform: scale(0);
    -webkit-transition: all 0.25s;
    transition: all 0.25s;
    .addSuccessInfo {
        padding: 1.5rem 0;
        .addSuccessInfoTrue {
            display: inline-block;
            width: 1.87rem;
            height: 1.87rem;
            margin-bottom: 0.81rem;
            background: url(../../../img/work-successTrue.png) center center no-repeat;
            background-size: contain;
        }
        .addSuccessTxt {
            color: #fff;
            font-weight: bold;
            font-size: 1rem;
        }
    }
}

.isok {
    -webkit-transform: scale(1);
    transform: scale(1);
    opacity: 1;
    pointer-events: none;
}


