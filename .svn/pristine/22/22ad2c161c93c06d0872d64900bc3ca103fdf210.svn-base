//结算详情页
.order-good-item {
    background-color: #fff;
    border-bottom: 1px solid #e6e6e6;
    border-top: 1px solid #e6e6e6;
    margin-bottom: 0.625rem;
    font-size: 1rem;
    .order-good-top {
        border-bottom: 1px solid #e6e6e6;
        padding: 0.625rem 0 0.625rem 0.9375rem;
        font-family: "黑体";
    }
    .mui-table-view-cell {
        font-size: 0.95rem;
        .mui-table {
            padding-bottom: 0.625rem;
        }
        .drawing-num {
            color: #FE7700;
        }
        .surplus-num {
            color: #EE0000;
        }
        .txtime {
            color: #999;
        }
    }
}
