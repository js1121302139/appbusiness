             color: #333;
             border-bottom: 0.125rem solid #333;
        }
    }
}
 input[type="text"] {
}
 input[type="tel"] {
}
 .optionDate {
     position: absolute;
     font-family: "微软雅黑";
     width: 90%;
     bottom: 1.875rem;
     top: 15.78125rem;
     left: 0;
     right: 0;
     margin: auto;
     margin-top: 1.25rem;
     padding-bottom: 0.31rem;
     overflow: scroll;
     -webkit-box-shadow: 0 0 0.9375rem 0.3125rem rgba(184, 184, 184, 0.4);
             box-shadow: 0 0 0.9375rem 0.3125rem rgba(184, 184, 184, 0.4);
     #scroll {
         position: absolute;
         z-index: 1;
         width: 100%;
         padding: 0;
    }
     .dateHeader {
         height: 2.81rem;
         line-height: 2.81rem;
         color: #333;
         display: -webkit-box;
         display: -ms-flexbox;
         display: flex;
         -webkit-box-flex: 1;
         -ms-flex: 1;
         flex: 1;
         width: 100%;
         .headerM {
             -webkit-box-flex: 1;
             -ms-flex: 1;
             flex: 1;
             font-size: 1rem;
        }
         .next {
             width: 2.75rem;
             background: url("../../../img/activityMNext-ico.png")center center no-repeat;
             background-size: 1rem 1rem;
        }
         .pre {
             width: 2.75rem;
             background: url("../../../img/activityMPre-ico.png")center center no-repeat;
             background-size: 1rem 1rem;
        }
    }
     .weekLsit {
         display: -webkit-box;
         display: -ms-flexbox;
         display: flex;
         -webkit-box-flex: 1;
         -ms-flex: 1;
         flex: 1;
         width: 100%;
         font-size: 0.875rem;
         color: #999;
         .weekLsit-item {
             -webkit-box-flex: 1;
             -ms-flex: 1;
             flex: 1;
             font-weight: bold;
             padding: 1.09rem 0;
        }
    }
     .dayLsit {
         .dayList-item {
             position: relative;
             width: 100% / 7;
             height: 3rem;
             line-height: 3rem;
             float: left;
             color: #4f5f6f;
        }
         .active {
             text-align: center;
             color: #fff;
             border-radius: 0.18rem;
             background: transparent;
             position: relative;
             z-index: 1;
            /* -webkit-box-shadow: 0 0 0 0.0625rem #838f9a;
             box-shadow: 0 0 0 0.0625rem #838f9a;
             */
             &:after {
                 content: " ";
                 z-index: -1;
                 min-width: 0.93rem;
                 width: 1.4rem;
                 min-height: 0.93rem;
                 height: 1.4rem;
                 border-radius: 50%;
                 padding: 0.5rem;
                 display: block;
                 background: #D7B374;
                 position: absolute;
                 top: 0;
                 right: 0;
                 bottom: 0;
                 left: 0;
                 margin: auto;
            }
        }
    }
     .roomNum {
         display: -webkit-box;
         display: -ms-flexbox;
         display: flex;
         -webkit-box-flex: 1;
             -ms-flex: 1;
                 flex: 1;
         height: 3.43rem;
         line-height: 0.21rem;
         padding: 0 0.9375rem;
         img{
             width: 1rem;
             height: 1rem;
             margin-left: 0.3125rem;
        }
         p {
             display: -webkit-box;
             display: -ms-flexbox;
             display: flex;
             -webkit-box-flex: 1;
                 -ms-flex: 1;
                     flex: 1;
             -webkit-box-align: center;
                 -ms-flex-align: center;
                     align-items: center;
             -webkit-box-pack: center;
                 -ms-flex-pack: center;
                     justify-content: center;
             width: 100%;
             border-top: 0.0625rem solid #CCCCCC;
        }
    }
}
/*活动列表*/
 @BascicColor: #007aff;
 @OrangeColor: #FE6606;
 @grayColor: #696766;
 .wrappers {
     position: absolute;
     top:15.5rem;
     bottom: 0rem;
     width: 100%;
     .wrapperList {
         background-color: #fff;
         height: 100%;
         ul{
             padding: 0 0.75rem;
        }
         .pullDown {
             display: none;
        }
         .ListItem {
             display: -webkit-box;
             display: -ms-flexbox;
             display: flex;
             -webkit-box-flex: 1;
                 -ms-flex: 1;
                     flex: 1;
             width: 100%;
             height: 5.1875rem;
             padding: 1.1875rem 0;
             background-color: #fff;
             border-bottom: 0.0625rem solid #e5e5e5;
             .activityInfo {
                 width: 17.375rem;
                 p {
                     font-size: 0.8125rem;
                     color: #999;
                }
                 p:first-child {
                     font-size: 0.9375rem;
                     margin-bottom: 1rem;
                     color: #333;
                     span {
                         color: #D7B374;
                    }
                }
            }
             .stateBg5 {
                 background: url("../../../images/stateBg1.png") center center no-repeat;
                 p, a {
                     display: none !important;
                }
            }
             .stateBg2 {
                 background: url("../../../images/stateBg2.png") center center no-repeat;
                 p, a {
                     display: none !important;
                }
            }
             .activityState {
                 width: 4.125rem;
                 height: 3.125rem;
                 background-size: contain;
                 p {
                     font-size: 0.875rem;
                     color: #D7B374;
                }
                 a {
                     display: inline-block;
                     width: 3.0625rem;
                     height: 1.375rem;
                     margin-top: 12.0.3125rem;
                     line-height: 1.375rem;
                     border: 1px solid #333;
                     border-radius: 0.25rem;
                     font-size: 0.8125rem;
                     color: #333;
                }
            }
        }
    }
}
/*弹出框*/
