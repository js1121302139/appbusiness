@import "../common";
 .topBar("../../../img/topBack-ico.png");
 .isNoData(0rem,0rem);
 html, body, #activityCenter {
     min-height: 100%;
     height: 100%;
     overflow: hidden;
}
 .Topactivity {
     color: #000000 !important;
}
 .activityBg {
     height: 8.12rem;
     background: url(../../../images/activityCenterBg.png) left top no-repeat;
     background-size: 100% 100%;
}
 #segmentedControl {
     background: #fff;
     .mui-control-item {
         border-bottom: none;
         span {
             color: #999;
             display: inline-block;
        }
    }
