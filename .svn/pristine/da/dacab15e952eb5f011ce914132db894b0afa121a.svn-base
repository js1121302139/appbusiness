.ListEnd span {
  display: inline-block;
}
.ListEnd span:after {
  content: " ";
  display: block;
  width: 5.125rem;
  height: 0.0625rem;
  position: absolute;
  left: 0.8125rem;
  top: 0;
  bottom: 0;
  margin: auto;
  background-color: #e8e8e8;
}
.ListEnd span:before {
