@import "../common";
.topBar("../../../img/topBack-ico.png");
#map {
    width: 100%;
    position: fixed;
    top: 64px;
    bottom: 3.125rem;
    text-align: center;
    background: #FFFFFF;
    z-index: 9;
}

.wrapperList {
    position: absolute;
    top: 0;
    bottom: 0;
    z-index: 999 !important;
}
.sure{
    position: fixed;
    bottom: 0;
    width: 100%;
    height: 3.125rem;
    border: 0;
    border-radius: 0;
    background: #222;
    font-size: 1rem;
    color: #fff;
    padding: 0;
    margin: 0; 
}
