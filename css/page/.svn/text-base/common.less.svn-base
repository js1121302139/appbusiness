@BascicColor: #0B84F0;
@BtnColorDeepAfter: #0D80EF;
@BtnColorDeepBefore: #0AADFE;
@BtnColorLightAfter: #ff9d17;
@BtnColorLightBefore: #ffcf18;
@BorderColor: #e5e5e5;
body,
html {
    background-color: #f5f5f5;
}

.mui-content {
    font-size: 0.87rem;
    color: #333;
    background-color: #f5f5f5;
}

.mui-bar {
    background-color: #fff;
    a {
        color: #666666;
    }
}

.mui-bar-tab .mui-tab-item.mui-active {
    color: #929292;
}

.mui-bar .mui-btn-link {
    color: #333;
}

.btn-gradient {
    padding: 0.625rem;
    background: -moz-linear-gradient(top, @BtnColorDeepBefore, @BtnColorDeepAfter);
    background: linear-gradient(top, @BtnColorDeepBefore, @BtnColorDeepAfter);
    background: -webkit-linear-gradient(top, @BtnColorDeepBefore, @BtnColorDeepAfter);
}

.btn-delete {
    padding: 0.625rem;
    background: -moz-linear-gradient(top, #f15d5d, #f10101);
    background: linear-gradient(top, #f15d5d, #f10101);
    background: -webkit-linear-gradient(top, #f15d5d, #f10101);
}

a {
    color: #333;
}

.mui-input-group .mui-input-row:after {
    left: 0px;
}

input {
    font-size: 0.87rem;
    -webkit-appearance: none;
}

.mui-input-group:before,
.mui-input-group:after {
    background-color: #FFFFFF;
}

.mui-btn-blue.mui-active:enabled,
.mui-btn-blue:enabled:active,
.mui-btn-primary.mui-active:enabled,
.mui-btn-primary:enabled:active,
input[type=submit].mui-active:enabled,
input[type=submit]:enabled:active {
    color: #fff;
    border: 1px solid @BascicColor;
    background-color: @BascicColor;
}

//商城商品
.shop-good-box {
    width: 100%;
    margin-top: 0.62rem;
    .shop-good-box-title {
        font-size: 0rem;
        img {
            max-width: 100%;
            vertical-align: bottom;
        }
    }
    .mui-table-view.mui-grid-view {
        padding: 0rem;
        .mui-table-view-cell {
            padding: 0rem;
            margin-right: 0rem;
            border-bottom: 1px #f5f5f5 solid;
            border-right: 1px #f5f5f5 solid;
        }
        .shop-good-item-box {
            background-color: #fff;
            padding: 0.62rem 0.18rem;
        }
    }
    .mui-table-view:after,
    .mui-table-view:before {
        background-color: #f5f5f5;
    }
    ul {
        background-color: #f5f5f5;
        li {
            .shop-good-title {
                width: 100%;
                text-align: left;
            }
            img {
                width: 100%;
                vertical-align: bottom;
            }
            span {
                font-size: 0.75rem;
                min-height: 2.75rem;
            }
            .shop-good-bottom-box {
                margin-top: 0.31rem;
                line-height: 1.75rem;
                width: 100%;
                height: 1.75rem;
                .shop-good-price {
                    float: left;
                    padding: 0rem;
                    color: #333;
                    font-weight: bold;
                }
                .shop-good-btn {
                    padding: 0rem 0.31rem;
                    float: right;
                    min-width: 4.25rem;
                    color: #fff;
                    border-radius: 0.5rem;
                    font-size: 0.75rem;
                }
                .shop-good-delete {
                    text-decoration: line-through;
                }
                .shop-good-btn-green {
                    background: -moz-linear-gradient(top, #11ca08, #079600);
                    background: linear-gradient(top, #11ca08, #079600);
                    background: -webkit-linear-gradient(top, #11ca08, #079600);
                }
                .shop-good-btn-orange {
                    background: -moz-linear-gradient(top, #ffad00, #fe7f00);
                    background: linear-gradient(top, #ffad00, #fe7f00);
                    background: -webkit-linear-gradient(top, #ffad00, #fe7f00);
                }
                .shop-good-btn-blue {
                    background: -moz-linear-gradient(top, #7ac2ff, #1c85df);
                    background: linear-gradient(top, #7ac2ff, #1c85df);
                    background: -webkit-linear-gradient(top, #7ac2ff, #1c85df);
                }
                .shop-good-btn-navy {
                    background: -moz-linear-gradient(top, #32dd9b, #06b16f);
                    background: linear-gradient(top, #32dd9b, #06b16f);
                    background: -webkit-linear-gradient(top, #32dd9b, #06b16f);
                }
                .shop-good-btn-pink {
                    background: -moz-linear-gradient(top, #fe82c9, #e00982);
                    background: linear-gradient(top, #fe82c9, #e00982);
                    background: -webkit-linear-gradient(top, #fe82c9, #e00982);
                }
                .shop-good-btn-yellow {
                    background: -moz-linear-gradient(top, #ffcc83, #eba104);
                    background: linear-gradient(top, #ffcc83, #eba104);
                    background: -webkit-linear-gradient(top, #ffcc83, #eba104);
                }
                .shop-good-btn-violet {
                    background: -moz-linear-gradient(top, #d77dfd, #9f04df);
                    background: linear-gradient(top, #d77dfd, #9f04df);
                    background: -webkit-linear-gradient(top, #d77dfd, #9f04df);
                }
                .shop-good-btn-gray {
                    background: -moz-linear-gradient(top, #f5f5f5, #ccc);
                    background: linear-gradient(top, #f5f5f5, #ccc);
                    background: -webkit-linear-gradient(top, #f5f5f5, #ccc);
                }
            }
        }
    }
}

.mui-content>.mui-table-view:first-child {
    margin-top: 0rem;
}

.ico-work {
    background: url(../../img/work_footer-01.png) center center no-repeat;
    background-size: cover;
}

.ico-order {
    background: url(../../img/work_footer-02.png) center center no-repeat;
    background-size: cover;
}

.ico-account {
    background: url(../../img/work_footer-03.png) center center no-repeat;
    background-size: cover;
}

.mui-table-view:before,
.mui-table-view:after {
    background-color: #fff;
}

.mui-bar-tab .mui-tab-item.mui-active {
    color: #5c9df4;
}

.mui-bar-tab .mui-tab-item {
    text-align: center;
    border-top: 1px solid #000;
    .mui-tab-label {
        color: #333;
    }
}

.bgImg {
    background: url(../../img/work_footer-01.png) center center no-repeat;
    background-position-y: -2.0625rem;
}

.bgImg1 {
    background: url(../../img/work_footer-02.png) center center no-repeat;
    background-position-y: -2.0625rem;
}

.bgImg2 {
    background: url(../../img/work_footer-03.png) center center no-repeat;
    background-position-y: -2.0625rem;
}

.mui-bar-tab .mui-tab-item .mui-icon {
    width: 2.06rem;
    height: 2.06rem;
    transition: all 0.5s;
    background-size: 2.0625rem 4.12rem;
}

.mui-bar img {
    width: 1.75rem;
    display: block;
    margin: 0 auto;
    text-align: center;
}

.mui-active .mui-icon {
    background-position-y: 0rem;
}


/*resetcss*/

html,
body,
div,
span,
applet,
object,
iframe,
h1,
h2,
h3,
h4,
h5,
h6,
p,
blockquote,
pre,
a,
abbr,
acronym,
address,
big,
cite,
code,
del,
dfn,
em,
img,
ins,
kbd,
q,
s,
samp,
small,
strike,
strong,
sub,
sup,
tt,
var,
b,
u,
i,
center,
dl,
dt,
dd,
ol,
ul,
li,
fieldset,
form,
label,
legend,
table,
caption,
tbody,
tfoot,
thead,
tr,
th,
td,
article,
aside,
canvas,
details,
embed,
figure,
figcaption,
footer,
header,
hgroup,
menu,
nav,
output,
ruby,
section,
summary,
time,
mark,
audio,
video {
    font-family: Arial !important;
    margin: 0;
    padding: 0;
    border: 0;
    font-size: 100%;
    font: inherit;
    vertical-align: baseline;
    cursor: pointer;
}


/* HTML5 display-role reset for older browsers */

article,
aside,
details,
figcaption,
figure,
footer,
header,
hgroup,
menu,
nav,
section {
    display: block;
}

body {
    line-height: 1;
}

ul {
    list-style: none;
}

blockquote,
q {
    quotes: none;
}

blockquote:before,
blockquote:after,
q:before,
q:after {
    content: '';
    content: none;
}

table {
    border-collapse: collapse;
    border-spacing: 0;
}


/*resetcssEnd*/

.wh(@width, @height) {
    width: @width;
    height: @height;
}


/*重构的顶部*/

.topBar(@url) {
    .topBar {
        padding-top: 1.25rem;
        height: 4rem;
        box-shadow: 0 0.0625rem 0.375rem #ccc;
        background-color: #fff;
        .topBack-ico {
            display: inline-block;
            .wh(2.75rem, 2.75rem);
            background: url(@url) left center no-repeat; 
            background-size: 1.25rem 1.25rem;
        }
        .mui-title {
            font-size: 1.12rem; 
            color: #333;
            z-index: -1;
        }
        .rightIco {
            display: block;
            width: 2.75rem;
            height: 2.75rem;
        }
    }
    .mui-content {
        padding-top: 4rem !important;
    }
}


/*顶部的卡片视图*/

.mine-top-box(@url, @backurl) {
    .minetop-box {
        .wh(100%, 14.6875rem);
        padding-top: 1.25rem;
        background: url(@url) left top no-repeat;
        background-size: cover;
        display: flex;
        flex-direction: column;
        * {
            color: #fff;
        }
        .topBar {
            display: flex;
            .wh(100%, 1.25rem);
            padding: 0 1.25rem;
            margin-top: 0.75rem; 
            justify-content: space-between;
            #myDetailed {
                font-size: 0.87rem;
                line-height: 1.25rem;
            }
            div:first-child {
                .wh(2.5rem, 2.5rem);
                background: url(@backurl) left top no-repeat;
                background-size: 1.25rem 1.25rem;
            }
        }
        .mine-topInfo {
            display: flex;
            margin-top: 1.25rem;
            flex-direction: column;
            .topInfoTit {
                font-size: 1.125rem;
                margin-bottom: 0.75rem;
            }
            .topInfoCountMoney {
                font-size: 2.5625rem;
            }
        }
        .mine-topInfoMoney {
            margin-top: 2.5625rem;
            display: flex;
            .wh(100%, 1.96875rem);
            div {
                position: relative;
                flex: 1;
                &:before {
                    position: absolute;
                    content: " ";
                    display: block;
                    .wh(0.0625rem, 1.875rem);
                    right: -0.0625rem;
                    top: 0;
                    bottom: 0;
                    margin: auto;
                    background-color: #fff;
                }
                p {
                    font-size: 0.75rem;
                }
                .money {
                    margin-top: 0.46875rem; 
                    font-size: 1rem;
                }
            }
        }
    }
}

/*没有数据*/
.isNoData(@top,@bottom){
    .isNoData {
    position: absolute;
    width: 100%;
    top: @top;
    bottom: @bottom;
    z-index: 9999;
    background: #fff;
    transform: scale(0);
    transition: all 0.25s;
    .isNoDataInfo {
        position: absolute;
        top: 8.125rem;
        left: 0;
        right: 0;
        margin: 0 auto;
        .isNoDataBg {
            margin: 0 auto;
            width: 4.21875rem;
            height: 8.125rem;
            margin-bottom: 0.9375rem;
            background: url(../../../images/work-message-nomsgBg.png) left top no-repeat;
            background-size: cover;
        }
        p{
            margin-bottom: 0.625rem;
            font-size: 0.6875rem;
            color: #999;
        }
        .isNoDataTit{
            font-size: 0.875rem;
            color: #8d8e92;
        }
    }
}
}
