@import "../common";

.topBar("../../../img/topBack-ico.png");
/*顶部*/
.mui-bar {
  .addSonAccNum {
    color: #333;
    font-size: 0.93rem;
  }
}

/*列表区域*/

.wrapperList {
    top: 4rem;
  .pullDown {
    display: none;
  }
  .accountNull {
    position: relative;
    top: 2.5rem;
    height: 100%;
    width: 100%;
    z-index: 1000;
    background: url(../../../img/work-teamNull.png) center 222.5px no-repeat;
    background-size: 5.65rem;
    display: none;
    p {
      position: absolute; 
      top: 19.62rem;
      width: 100%;
      font-size: 0.93rem;
      color: #999;
    }
  }
  .mui-table-view-cell {
    &:after {
      display: block;
      left: 0;
    }
    .memberLeft {
      width: 60%;
      .membersHeadImg {
        width: 2.5rem;
        height: 100%;
        float: left;
        img {
          width: 100%;
          height: 2.5rem;
        }
      }
      .membersInfo {
        width: 75%;
        text-indent: 0.62rem;
        p {
          width: 100%;
          line-height: 1.87rem;
          font-size: 0.93rem;
          color: #333;
        }
      }
    }
    .memberRight {
      width: 7.18rem;
      float: right;
      .membersMoney {
        color: #D7B374;
        font-size: 0.96rem;
        margin-bottom: 2.5rem;
      }
      .mui-btn{
        .wh(49px,22px);
        line-height: 22px;
        padding: 0;
      }
      .btnJY {
        color: #666;
        border: 1px solid #666;
        transition: all .25s;
      }
      .btnEd {
        color: #fff;
        background-color: #333;
        border: 1px solid #333;
      }
      .activeBtn {

        border-color: #333;
        background-color: #333;
        color: #fff;
      }
    }
  }
}