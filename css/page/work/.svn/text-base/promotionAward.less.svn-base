@import "../common";
body {
    background-color: #f5f5f5;
}

.mine-top-box("../../../images/order-bg.png", "../../../img/topBackB-ico.png");
.minetop-box {
    height: 11.87rem;
}

.wrapperList {
    top: 12.5rem;
    background-color: #fff;
    .pullDown {
        display: none;
    }
    .wrapperListItem {
        position: relative;
        display: flex;
        flex: 1;
        height: 44px;
        line-height: 44px;
        padding: 0 15.5px 0 15px;
        justify-content: space-between;
        &:after {
            position: absolute;
            content: " ";
            display: block;
            .wh(95%, 1px);
            right: 0px;
            left: 0;
            bottom: -1px;
            margin: auto;
            background-color: #e5e5e5;
            transform: scaleY(0.5);
        }
        .ordersDate {
            font-size: 13px;
            color: #333;
        }
        .ordersMoney {
            font-size: 15px;
            color: #D7B374;
        }
    }
}

.footerButton {
    position: fixed;
    bottom: 0px;
    .wh(100%, 49px);
    line-height: 49px;
    background-color: #D7B374;
    z-index: 9999;
}