@import "../common";
body {
    background-color: #f5f5f5;
}

html,
body,#myincome{
    height: 100%;
    min-height: 100%;
    overflow: hidden;
}

.mine-top-box("../../../images/order-bg.png", "../../../img/topBackB-ico.png");

.wrappers {
    height: 100%;
    ul {
        padding-bottom: 15.31rem;
    }
    .wrapperList {
        background-color: #fff;
        .pullDown {
            display: none;
        }
        .pullUp {
            position: relative;
            top: -15.31rem;
        }
        .wrapperListItem {
            position: relative;
            display: flex;
            flex: 1;
            height: 44px;
            line-height: 44px;
            padding: 0 15.5px 0 15px;
            justify-content: space-between;
            &:after {
                position: absolute;
                content: " ";
                display: block;
                .wh(95%, 1px);
                right: 0px;
                left: 0;
                bottom: -1px;
                margin: auto;
                background-color: #e5e5e5;
                transform: scaleY(0.5);
            }
            .ordersDate {
                width: 282.5px;
                font-size: 13px;
                color: #333;
            }
            .ordersMoney {
                font-size: 15px;
                color: #D7B374;
            }
        }
    }
}

.footerButton {
    position: fixed;
    bottom: 0px;
    .wh(100%, 49px);
    line-height: 49px;
    background-color: #D7B374;
    z-index: 9999;
}