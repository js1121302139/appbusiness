@import "../common";
.mine-top-box("../../../images/order-bg.png", "../../../img/topBackB-ico.png");
.monthlyEarnings {
    font-size: 13px;
    color: #333;
    .wh(100%, 36px);
    padding-right: 25px;
    line-height: 36px;
    img {
        display: inline-block;
        .wh(16px, 16px);
        vertical-align: middle;
    }
}

html,
body,
#shareprofit {
    height: 100%;
    min-height: 100%;
    overflow: hidden;
}

.wrapperList {
    top:17rem;
    bottom: 3.125rem;
    background-color: #fff;
    .pullDown {
        display: none;
    }
    .ListItem {
        display: flex;
        flex: 1;
        position: relative;
        .wh(100%, 60px);
        padding: 13px 12px 0 12px;
        flex-direction: column;
        &:after {
            position: absolute;
            content: " ";
            display: block;
            .wh(95%, 1px);
            right: 0px;
            left: 0;
            bottom: -1px;
            margin: auto;
            background-color: #e5e5e5;
            transform: scaleY(0.5);
        }
        .ListItemTop {
            display: flex;
            justify-content: space-between;
            margin-bottom: 11px;
            font-size: 13px;
            color: #333;
            .itemInfo {
                font-size: 13px;
                color: #333;
            }
            .itemMoney {
                font-size: 15px;
                color: #D7B374;
            }
        }
        .ListItemDate {
            font-size: 12px;
            color: #999;
        }
    }
}

.footerButton {
    position: fixed;
    z-index: 9999;
    bottom: 0px;
    .wh(100%, 49px);
    color: #fff;
     font-size: 1.12rem;
         border: none;
    border-radius: 0;
    background-color: #D7B374;
}
.scale1{
    transform: scale(1)!important;
}
.isNoData(0rem,0rem);