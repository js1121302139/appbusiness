  color: #333;
  display: -webkit-box;
  display: -ms-flexbox;
  display: flex;
  -webkit-box-flex: 1;
  -ms-flex: 1;
  flex: 1;
  width: 100%;
}
.optionDate .dateHeader .headerM {
  -webkit-box-flex: 1;
  -ms-flex: 1;
  flex: 1;
  font-size: 16px;
}
.optionDate .dateHeader .next {
  width: 2.75rem;
  background: url("../../../img/activityMNext-ico.png") center center no-repeat;
  background-size: 16px 16px;
}
.optionDate .dateHeader .pre {
  width: 2.75rem;
  background: url("../../../img/activityMPre-ico.png") center center no-repeat;
  background-size: 16px 16px;
}
.optionDate .weekLsit {
  display: -webkit-box;
  display: -ms-flexbox;
  display: flex;
  -webkit-box-flex: 1;
  -ms-flex: 1;
  flex: 1;
  width: 100%;
  font-size: 14px;
  color: #999;
}
.optionDate .weekLsit .weekLsit-item {
  -webkit-box-flex: 1;
  -ms-flex: 1;
  flex: 1;
  font-weight: bold;
  padding: 1.09rem 0;
}
.optionDate .dayLsit .dayList-item {
  position: relative;
  width: 14.28571429%;
  height: 48px;
  line-height: 48px;
  float: left;
  color: #4f5f6f;
}
.optionDate .dayLsit .active {
  text-align: center;
  color: #fff;
  border-radius: 0.18rem;
  background: transparent;
  position: relative;
  z-index: 1;
  /*      -webkit-box-shadow: 0 0 0 1px #838f9a;
      box-shadow: 0 0 0 1px #838f9a;*/
}
.optionDate .dayLsit .active:after {
  content: " ";
  z-index: -1;
  min-width: 0.93rem;
  width: 1.4rem;
  min-height: 0.93rem;
  height: 1.4rem;
  border-radius: 50%;
  padding: 0.5rem;
  display: block;
  background: #D7B374;
  position: absolute;
  top: 0;
  right: 0;
  bottom: 0;
  left: 0;
  margin: auto;
}
.optionDate .dateFooter {
  margin-bottom: 1.87rem;
}
.optionDate .dateFooter p {
  font-size: 0.75rem;
  color: #333;
  margin-bottom: 0px;
}
.optionDate .dateFooter .dateFooter-footer {
