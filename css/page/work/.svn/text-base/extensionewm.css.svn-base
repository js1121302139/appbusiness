.mui-content .extensionewm-top {
  position: relative;
  margin-top: 15px;
  height: 35px;
  line-height: 23px;
  border-bottom: 1px solid #e6e6e6;
}
.mui-content .extensionewm-top .extensionewmCon {
  height: 35px;
  background-image: url("../../../img/tg-ewm.png");
  background-repeat: no-repeat;
  background-size: 25px 25px;
  display: inline-block;
  font-size: 1rem;
  margin-left: 15px;
}
.mui-content .extensionewm-top .extensionewmCon .extensionewmConNum {
  margin-left: 30px;
}
.mui-content .extensionewm-top button {
  background-color: #0A93F6;
  color: #fff;
  padding: 2px 15px;
  border-radius: 5px;
  float: right;
  margin-right: 15px;
}
.mui-content .extensionewm-list ul li {
  height: 80px;
  border-top: 1px solid #ebe6e6;
}
.mui-content .extensionewm-list ul li .extensionewm-left {
  position: relative;
}
.mui-content .extensionewm-list ul li .extensionewm-left img {
  position: absolute;
  width: 50px;
  left: 3px;
  top: 9px;
}
.mui-content .extensionewm-list ul li .extensionewm-right {
  height: 80px;
  text-align: left;
  font-size: 0.9rem;
}
.mui-content .extensionewm-list ul li .extensionewm-right .extensionewm-time {
  color: #8a8686;
}
.left-five {
  margin-left: 5px;
}
.mui-table-view.mui-grid-view {
  width: 100%;
  padding: 0 0 10px 0;
}
