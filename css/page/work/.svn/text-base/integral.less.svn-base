.integral-container {
    color: #000000;
    .integral-title {
        background-color: #FFFFFF;
        height: 50px;
        line-height: 50px;
        img {
            width: 1.2rem;
            position: relative;
            top: 5px;
            left: 8px;
        } 
    }
    .integral-den {
        padding-left: 3%;
        font-size: 0.85rem;
        .integral-num {
            font-size: 0.95rem;
            color: #0B84F0;
            margin-right: 0.2rem;
        }
    }
    .mui-table-view {
        .integral-info-left {
            width: 70%;
            display: inline-block;
            .font-1 {
                font-size: 0.9rem;
                color: #000000;
            }
            p {
                margin-bottom: 5px;
            }
            .mobile-num {
                margin-left: 5px;
            }
        }
        .integral-info-right {
            width: 28%;
            display: inline-block;
            p {
                font-size: 1.3rem;
                text-align: right;
                padding-bottom: 12px;
            }
            .font-1 {
                font-size: 1rem;
            }
        }
        .color-z {
            p {
                color: #FE7700;
            }
        }
        .color-s {
            p {
                color: #0B84F0;
            }

        }
    }
}
