@import "../common";
.topBar("../../../img/topBack-ico.png");
html,
body,
#message {
    min-height: 100%;
    height: 100%;
    margin-top: 10px;
    background: #f2f2f2;
}

.msgheader {
    -webkit-box-shadow: none;
    box-shadow: none;
    #TopMsg {
        color: #000000 !important;
    }
}

.wrappers {
    position: relative;
    height: 100%;
    top:2.75rem ;
    .wrapperList {
        .pullDown {
            display: none;
        }
        .pullUp {
            display: none !important;
            position: relative;
            top:-2.75rem ;
        }
        ul {
            display: flex;
            flex-direction: column;
            align-items: center;
            padding:0 0.75rem 5.75rem 0.75rem ;
            
            .ListItem {
                display: flex;
                flex: 1;
                width: 100%;
                margin-top: 20px;
                flex-direction: column;
                .msgDate {
                    padding: 0px 0 15px 0;
                    background-color: #f2f2f2;
                    font-size: 0.75rem;
                }
                .msgInfo {
                     background-color: #fff;
                    border-radius: 0.37rem;
                    padding: 20px 12px;
                    * {
                        color: #333333;
                    }
                    .msgTop {
                        display: flex;
                        flex: 1;
                        align-items: center;
                        justify-content: space-between;
                        background: url("../../../img/order-ListGo-ico.png") right center no-repeat;
                        background-size: 15px 15px;
                        .msgTit {
                            font-size: 15px;
                        }
                        .msg-ico {
                            display: flex;
                            align-items: center;
                            margin-right: 15px;
                            .msgBadge {
                                display: inline-block;
                                .wh(5px,
                                5px);
                                border-radius: 2.5px;
                                background-color: #cea055;
                            }
                        }
                    }
                    .msgimg {
                        margin-top: 14px;
                        img {
                            width: 100%;
                            height: 9.37rem;
                        }
                    }
                    .magText {
                        margin-top: 15px;
                        font-size: 13px;
                    }
                }
            }
        }
    }
}


/*暂无消息*/

.msgListNull {
    background-size: 4.4rem 6.5rem !important;
    background-position-y: 8rem !important;
}

.messageNull {
    color: #999;
    font-size: 0.93rem;
    background: transparent;
    border: 0;
    margin-top: 14.9rem;
    display: none;
}


