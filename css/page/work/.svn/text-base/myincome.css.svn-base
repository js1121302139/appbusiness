body,
html {
  background-color: #f5f5f5;
}
.mui-content {
  overflow: hidden;
  font-size: 0.87rem;
  color: #333;
  background-color: #f5f5f5;
}
.mui-bar {
  background-color: #fff;
}
.mui-bar a {
