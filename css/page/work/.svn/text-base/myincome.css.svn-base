.shop-good-box ul li .shop-good-bottom-box .shop-good-btn-navy {
  background: -moz-linear-gradient(top, #32dd9b, #06b16f);
  background: linear-gradient(top, #32dd9b, #06b16f);
  background: -webkit-linear-gradient(top, #32dd9b, #06b16f);
}
.shop-good-box ul li .shop-good-bottom-box .shop-good-btn-pink {
  background: -moz-linear-gradient(top, #fe82c9, #e00982);
  background: linear-gradient(top, #fe82c9, #e00982);
  background: -webkit-linear-gradient(top, #fe82c9, #e00982);
}
.shop-good-box ul li .shop-good-bottom-box .shop-good-btn-yellow {
  background: -moz-linear-gradient(top, #ffcc83, #eba104);
  background: linear-gradient(top, #ffcc83, #eba104);
  background: -webkit-linear-gradient(top, #ffcc83, #eba104);
}
.shop-good-box ul li .shop-good-bottom-box .shop-good-btn-violet {
  background: -moz-linear-gradient(top, #d77dfd, #9f04df);
  background: linear-gradient(top, #d77dfd, #9f04df);
  background: -webkit-linear-gradient(top, #d77dfd, #9f04df);
}
.shop-good-box ul li .shop-good-bottom-box .shop-good-btn-gray {
  background: -moz-linear-gradient(top, #f5f5f5, #ccc);
  background: linear-gradient(top, #f5f5f5, #ccc);
  background: -webkit-linear-gradient(top, #f5f5f5, #ccc);
}
.mui-content > .mui-table-view:first-child {
  margin-top: 0rem;
}
.ico-work {
  background: url(../../img/work_footer-01.png) center center no-repeat;
  background-size: cover;
}
.ico-order {
  background: url(../../img/work_footer-02.png) center center no-repeat;
  background-size: cover;
}
.ico-account {
  background: url(../../img/work_footer-03.png) center center no-repeat;
  background-size: cover;
}
.mui-table-view:before,
.mui-table-view:after {
  background-color: #fff;
}
.mui-bar-tab .mui-tab-item.mui-active {
  color: #5c9df4;
}
.mui-bar-tab .mui-tab-item {
  text-align: center;
  border-top: 1px solid #000;
}
.mui-bar-tab .mui-tab-item .mui-tab-label {
  color: #333;
}
.bgImg {
  background: url(../../img/work_footer-01.png) center center no-repeat;
  background-position-y: -33px;
}
.bgImg1 {
  background: url(../../img/work_footer-02.png) center center no-repeat;
  background-position-y: -33px;
}
.bgImg2 {
  background: url(../../img/work_footer-03.png) center center no-repeat;
  background-position-y: -33px;
}
.mui-bar-tab .mui-tab-item .mui-icon {
  width: 2.06rem;
  height: 2.06rem;
  transition: all 0.5s;
  background-size: 33px 4.12rem;
}
.mui-bar img {
  width: 1.75rem;
  display: block;
  margin: 0 auto;
  text-align: center;
}
.mui-active .mui-icon {
  background-position-y: 0rem;
}
/*resetcss*/
html,
body,
div,
span,
applet,
object,
iframe,
h1,
h2,
h3,
h4,
h5,
h6,
p,
blockquote,
pre,
a,
abbr,
acronym,
address,
big,
cite,
code,
del,
dfn,
em,
img,
ins,
kbd,
q,
s,
samp,
small,
strike,
strong,
sub,
sup,
tt,
var,
b,
u,
i,
center,
dl,
dt,
dd,
ol,
ul,
li,
fieldset,
form,
label,
legend,
table,
caption,
tbody,
tfoot,
thead,
tr,
th,
td,
article,
aside,
canvas,
details,
embed,
figure,
figcaption,
footer,
header,
hgroup,
menu,
nav,
output,
ruby,
section,
summary,
time,
mark,
audio,
video {
  font-family: Arial !important;
  margin: 0;
  padding: 0;
  border: 0;
  font-size: 100%;
  font: inherit;
  vertical-align: baseline;
  cursor: pointer;
}
/* HTML5 display-role reset for older browsers */
article,
aside,
details,
figcaption,
figure,
footer,
header,
hgroup,
menu,
nav,
section {
  display: block;
}
body {
  line-height: 1;
}
ul {
  list-style: none;
}
blockquote,
q {
  quotes: none;
}
blockquote:before,
blockquote:after,
q:before,
q:after {
  content: '';
  content: none;
}
table {
  border-collapse: collapse;
  border-spacing: 0;
}
/*resetcssEnd*/
/*重构的顶部*/
/*顶部的卡片视图*/
body {
  background-color: #f5f5f5;
}
html,
body,
#myincome {
  height: 100%;
  min-height: 100%;
  overflow: hidden;
}
.minetop-box {
  width: 100%;
  height: 235px;
  padding-top: 1.25rem;
  background: url("../../../images/order-bg.png") left top no-repeat;
  background-size: cover;
  display: flex;
  flex-direction: column;
}
.minetop-box * {
  color: #fff;
}
.minetop-box .topBar {
  display: flex;
  width: 100%;
  height: 20px;
  padding: 0 20px;
  margin-top: 12px;
  justify-content: space-between;
}
.minetop-box .topBar div:first-child {
  width: 40px;
  height: 40px;
  background: url("../../../img/topBackB-ico.png") left center no-repeat;
  background-size: 20px 20px;
}
.minetop-box .mine-topInfo {
  display: flex;
  margin-top: 1.25rem;
  flex-direction: column;
}
.minetop-box .mine-topInfo .topInfoTit {
  font-size: 18px;
  margin-bottom: 12px;
}
.minetop-box .mine-topInfo .topInfoCountMoney {
  font-size: 41px;
}
.minetop-box .mine-topInfoMoney {
  margin-top: 41px;
  display: flex;
  width: 100%;
  height: 31.5px;
}
.minetop-box .mine-topInfoMoney div {
  position: relative;
  flex: 1;
}
.minetop-box .mine-topInfoMoney div:before {
  position: absolute;
  content: " ";
  display: block;
  width: 1px;
  height: 30px;
  right: -1px;
  top: 0;
  bottom: 0;
  margin: auto;
  background-color: #fff;
}
.minetop-box .mine-topInfoMoney div p {
  font-size: 12px;
}
.minetop-box .mine-topInfoMoney div .money {
  margin-top: 7.5px;
  font-size: 16px;
}
.wrappers {
  height: 100%;
}
.wrappers ul {
  padding-bottom: 15.31rem;
}
.wrappers .wrapperList {
  background-color: #fff;
}
.wrappers .wrapperList .pullDown {
  display: none;
}
.wrappers .wrapperList .pullUp {
  position: relative;
  top: -15.31rem;
}
.wrappers .wrapperList .wrapperListItem {
  position: relative;
  display: flex;
  flex: 1;
  height: 44px;
  line-height: 44px;
  padding: 0 15.5px 0 15px;
  justify-content: space-between;
}
.wrappers .wrapperList .wrapperListItem:after {
  position: absolute;
  content: " ";
  display: block;
  width: 95%;
  height: 1px;
  right: 0px;
  left: 0;
  bottom: -1px;
  margin: auto;
  background-color: #e5e5e5;
  transform: scaleY(0.5);
}
.wrappers .wrapperList .wrapperListItem .ordersDate {
  width: 282.5px;
  font-size: 13px;
  color: #333;
}
.wrappers .wrapperList .wrapperListItem .ordersMoney {
  font-size: 15px;
  color: #D7B374;
}
.footerButton {
  position: fixed;
  bottom: 0px;
  width: 100%;
  height: 49px;
  line-height: 49px;
  background-color: #D7B374;
  z-index: 9999;
}
