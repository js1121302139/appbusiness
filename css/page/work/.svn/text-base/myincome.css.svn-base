}
.mui-bar-tab .mui-tab-item.mui-active {
  color: #929292;
}
.mui-bar .mui-btn-link {
  color: #333;
}
.btn-gradient {
  padding: 10px;
  background: -moz-linear-gradient(top, #0AADFE, #0D80EF);
  background: linear-gradient(top, #0AADFE, #0D80EF);
  background: -webkit-linear-gradient(top, #0AADFE, #0D80EF);
}
.btn-delete {
  padding: 10px;
  background: -moz-linear-gradient(top, #f15d5d, #f10101);
  background: linear-gradient(top, #f15d5d, #f10101);
  background: -webkit-linear-gradient(top, #f15d5d, #f10101);
}
a {
