@import "../common.less";
.topBar("../../../img/topBack-ico.png");
html,
body,
#partyAdmin {
    min-height: 100%;
    height: 100%;
    overflow: hidden;
}

.mui-content {
    height: 100%;
}

    .wrapperList {
        top:4rem;
        bottom: 3.125rem;
        .pullDown {
            display: none;
        }
        .listItem {
            display: flex;
            flex: 1;
            width: 100%;
            flex-direction: column;
            padding: 0.9375rem 0.75rem 0 0.75rem;
            margin-bottom: 0.625rem;
            background-color: #fff;
            * {
                font-size: 0.8125rem;
                color: #333;
            }
            .InfoTit {
                display: flex;
                flex: 1;
                width: 100%;
                justify-content: space-between;
                margin-bottom: 0.03125rem;
                .partyTit {
                    display: flex;
                    align-self: flex-end;
                    font-size: 1rem;
                }
                .partyState {
                    font-size: 0.875rem;
                    color: #D7B374;
                }
                .partyRoad {
                    display: flex;
                    flex: 1;
                    width: 100%;
                    justify-content: flex-start;
                    padding-left: 0.37rem;
                    span {
                        display: flex;
                        .wh(2.28rem, 0.96rem);
                        background: #D7B374;
                        align-items: center;
                        justify-content: center;
                        border-radius: 0.46rem 0.46rem 0.46rem 0;
                        font-size: 0.68rem;
                        color: #fff;
                    }
                }
            }
            .orderInfo {
                display: flex;
                flex: 1;
                width: 100%;
                flex-direction: row;
                justify-content: space-between;
                padding-bottom: 0.9375rem;
                .orderInfoL {
                    p {
                        margin-top: 0.8125rem;
                    }
                }
                .orderInfoR {
                    display: flex;
                    width: 4.375rem;
                    align-items: flex-end;
                    .orderContainer {
                        display: flex;
                        align-items: center;
                        flex-direction: column;
                        .wh(4.375rem, 4.375rem);
                        border-radius: 35px;
                        padding: 1rem 0 0.84375rem 0;
                        box-shadow: inset 0 0 0 0.1875rem #D7B374;
                        .orderContainerNum {
                            font-size: 1.3125rem;
                            color: #333;
                            margin-bottom: 0.18rem;
                        }
                        .orderContainerName {
                            font-size: 0.75rem;
                            color: #999;
                        }
                    }
                }
            }
            .itemBottom {
                display: flex;
                flex: 1;
                .wh(100%, 2.125rem);
                line-height: 2.09375rem;
                overflow: hidden;
                border-top: 1px solid #e5e5e5;
                div {
                    flex: 1;
                    position: relative;
                    &:after {
                        content: " ";
                        display: block;
                        position: absolute;
                        .wh(0.0625rem, 1.0625rem);
                        bottom: 0;
                        top: 0;
                        right: -0.0625rem;
                        margin: auto;
                        background-color: #e5e5e5;
                    }
                }
            }
        }
        .ListEnd {
            .wh(100%, 51.0.3125rem);
            position: relative;
            line-height: 3.21875rem;
            margin-top: -0.625rem;
            background-color: #f5f5f5;
            font-size: 0.75rem;
            color: #ccc;
            span {
                display: inline-block;
                &:after {
                    content: " ";
                    display: block;
                    .wh(5.125rem, 0.0625rem);
                    position: absolute;
                    left: 0.8125rem;
                    top: 0;
                    bottom: 0;
                    margin: auto;
                    background-color: #e8e8e8;
                }
                &:before {
                    content: " ";
                    display: block;
                    position: absolute;
                    .wh(5.125rem, 0.0625rem);
                    right: 0.8125rem;
                    top: 0;
                    bottom: 0;
                    margin: auto;
                    background-color: #e8e8e8;
                }
            }
        }
    }

footer {
    display: flex;
    flex: 1;
    position: fixed;
    z-index: 99999;
    justify-content: center;
    align-items: center;
    bottom: 0;
    .wh(100%, 3.125rem);
    color: #fff;
    div {
        display: flex;
        flex: 1;
        flex-direction: column;
        justify-content: center;
        &:after {
            content: " ";
            display: block;
            .wh(0.0625rem, 1.8125rem);
            position: absolute;
            right: 0;
        }
        .createParty-ico {
            background: url(../../../img/createParty-ico.png) center center no-repeat;
        }
        .partyScanner-ico {
            background: url(../../../img/partyScanner-ico.png) center center no-repeat;
        }
        .footerico {
            align-self: center;
            display: inline-block;
            .wh( 2.06rem, 2.06rem);
            background-size: 1.37rem 1.37rem;
        }
    }
    background-color: #222;
}