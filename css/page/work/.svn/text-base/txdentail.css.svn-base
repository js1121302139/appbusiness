.order-good-item {
  background-color: #fff;
  border-bottom: 1px solid #e6e6e6;
  border-top: 1px solid #e6e6e6;
  margin-bottom: 10px;
  font-size: 1rem;
}
.order-good-item .order-good-top {
  border-bottom: 1px solid #e6e6e6;
  padding: 10px 0 10px 15px;
  font-family: "黑体";
}
.order-good-item .mui-table-view-cell {
  font-size: 0.95rem;
}
.order-good-item .mui-table-view-cell .mui-table {
  padding-bottom: 10px;
}
.order-good-item .mui-table-view-cell .drawing-num {
  color: #FE7700;
}
.order-good-item .mui-table-view-cell .surplus-num {
  color: #EE0000;
}
.order-good-item .mui-table-view-cell .txtime {
  color: #999;
}
