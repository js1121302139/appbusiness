.mui-content .mui-table-view {
  margin-bottom: 20px;
}
.mui-content .mui-table-view .mui-table-view-cell .right-info {
  color: #686060;
  position: absolute;
  right: 40px;
}
.mui-plus .plus {
  display: inline;
}
.plus {
  display: none;
}
#bottomPopover {
  position: fixed;
  top: 78%!important;
  width: 92%;
  left: 4%!important;
}
#bottomPopover .mui-popover-arrow {
  left: auto;
  right: 6px;
}
p {
  text-indent: 22px;
}
span.mui-icon {
  font-size: 14px;
  color: #007aff;
  margin-left: -15px;
  padding-right: 10px;
}
.mui-popover {
  height: 120px;
}
.mui-content {
  padding: 10px;
}
.mui-popover .mui-popover-arrow:after {
  width: 0px;
  height: 0px;
}
.popup-input {
  width: 80%;
  margin: 10px auto 0;
}
.popup-button button {
  font-size: 16px!important;
  width: 80%;
  margin: 0 10%;
}
