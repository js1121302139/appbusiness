@import "../common";
.topBar("../../../img/topBack-ico.png");
.mui-content {
    margin-top: 0.62rem;
    .mui-table-view-cell { 
        &:after {
            position: absolute;
            right: 0;
            bottom: 0;
            left: 0px;
            height: 1px;
            content: '';
            -webkit-transform: scaleY(.5);
            transform: scaleY(.5);
            background-color: #c8c7cc;
        }
        .myLogin{
            background: url(../../../img/ico-safecenter-1.png) 1.09rem center no-repeat;
            background-size: 6% !important;
        }
        .myPlay{
            background: url(../../../img/ico-safecenter-2.png) 1.09rem center no-repeat;
        }
        .myWecath{
            background: url(../../../img/ico-safecenter-3.png) 1.09rem center no-repeat;
        }
        .myBankCard{
            background: url(../../../img/ico-safecenter-4.png) 1.09rem center no-repeat;
        }
        .mui-navigate-right{
           text-indent:3.12rem;
           background-size:1.375rem 1.375rem;
        }
        
    }
}